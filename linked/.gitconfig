[color]
	ui = auto
[core]
	excludesfile = ~/.gitignore
	attributesfile = ~/.gitattributes
	precomposeunicode = true
[tig "bind"]
	generic = g move-first-line
	generic = G move-last-line
[hub]
	protocol = https
[merge]
	ff = true
[merge "gemfilelock"]
	name = relocks the gemfile.lock
	driver = bundle install
[push]
	default = simple
[ghq]
  root = ~/src
